{
  "private": true,
  "workspaces": ["apps/**", "packages/**"],
  "scripts": {
    "co": "sui-mono commit",
    "dev": "turbo run dev --parallel",
    "dev:server": "pnpm --filter server run dev",
    "dev:web": "pnpm --filter web run dev",
    "build": "turbo run build",
    "build:server": "pnpm --filter server run build",
    "build:web": "pnpm --filter web run build",
    "prod:server": "pnpm --filter server run start:prod",
    "format": "rome format . --write",
    "lint": "rome check . --apply"
  },
  "devDependencies": {
    "@s-ui/mono": "2.35.0",
    "rome": "12.1.3",
    "turbo": "1.10.12"
  },
  "engines": {
    "pnpm": ">=8.6.5",
    "node": ">=18"
  },
  "packageManager": "pnpm@8.6.10"
}
