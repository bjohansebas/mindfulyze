{
  "private": true,
  "scripts": {
    "build": "prisma generate & next build",
    "co": "pnpm run format && sui-mono commit",
    "clean": "git clean -xdf node_modules",
    "dev": "next dev",
    "dev:db": "pnpm with-env prisma studio --port 5556",
    "db:docker:startup": "docker compose --profile db up -d",
    "db:docker:shutdown": "docker compose --profile db down",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate --accept-data-loss",
    "app:docker:startup": "pnpm i && docker compose --profile dev up -d",
    "app:docker:shutdown": "pnpm clean:workspaces && docker compose --profile dev down",
    "format": "rome format .",
    "format:fix": "rome format . --write",
    "lint": "rome check .",
    "lint:next": "next lint",
    "lint:fix": "rome check . --apply",
    "start": "next start",
    "with-env": "dotenv -e .env --"
  },
  "dependencies": {
